    <div class="container">
        <div class="table-wrapper tm-block tm-block-taller tm-block-scroll">
            <div class="table-title" >
                <div class=" d-flex justify-content-between">
                    <div class="">
						<h2>Order <b>Details</b></h2>
					</div>
					<div class="">
						<button type="button" style="background: transparent; border: none;" class="" onclick="return ExportToExcel()" ><img style="width: 30px;" src="https://cdn-icons-png.flaticon.com/512/9811/9811256.png" alt=""></button>
                        <button type="button" style="background: transparent; border: none;" class="btn-light" onclick="return printTable()" ><img style="width: 30px;" src="https://cdn-icons-png.flaticon.com/512/839/839286.png" alt=""> </button>
					</div>
                </div>
            </div>
			<div class="table-filter">
				<div class="row">
           
                    <form id="sales-filter">
                        <label for="filter">Filter by:</label>
                        <select id="filter" name="filter">
                          <option value="all">All Time</option>
                          <option value="weekly">This Week</option>
                          <option value="yearly">This Year</option>
                        </select>
                        <button type="submit">Generate Report</button>
                      </form>
                </div>
			</div>
            <table class="table" id="salesTable">
                <thead>
                    <tr>
                        <th>#</th>
                        <th>Order Id</th>
                        <th>Customer</th>
						<th>Location</th>
						<th>Order Date</th>						
						<th>Net Amount</th>
                    </tr>
                </thead>
                <tbody>
                  <%let x%>
                  <%x=(page-1)*10%>
                    <%if(sales!=0){%>
                        <%sales.forEach(sales => {%>
                          <%x+=1%>
                            <tr>
                               
                                <td><%=x%></td>
                                <td><%=sales.orderId%></td>
                                <td><%=sales.userId.username%></td>
                                <td><%=sales.location%></td>
                                <td><%=sales.date.toDateString()%></td>                        
                                <td><%=sales.totalSales%></td>
                            </tr>
                        <%})%>
                        
                        <%}else{%>
                            <tr>
                                <td>null</td>
                            </tr>
                            <%}%>
                    
                </tbody>
            </table>
			<div class="clearfix mt-2">
        <%for(let j=1;j<=totalPages;j++){%>
                <!-- <div class="hint-text">Showing <b><%=j%></b> out of <b><%=count %>></b> entries</div>
                <ul class="pagination"> -->
                  <a class="text-white btn-sm btn-secondary" href="?page=<%=j%>"><%=j%></a>
